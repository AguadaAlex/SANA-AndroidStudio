<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sanapruebados">

    <uses-feature android:name="android.hardware.camera2" android:required="false"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
   <!-- <uses-permission android:name="android.permission.CAMERA" />-->


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".altaMomentos"></activity>
        <activity
            android:name=".MDetalle.MomentoListActivity"
            android:label="@string/title_momento_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MDetalle.MomentoDetailActivity"
            android:label="@string/title_momento_detail"
            android:parentActivityName=".MDetalle.MomentoListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.sanapruebados.MDetalle.MomentoListActivity" />
        </activity>
        <activity
            android:name=".MDetalle.EstablecimientoListActivity"
            android:label="@string/title_establecimiento_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MDetalle.EstablecimientoDetailActivity"
            android:label="@string/title_establecimiento_detail"
            android:parentActivityName=".MDetalle.EstablecimientoListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.sanapruebados.MDetalle.EstablecimientoListActivity" />
        </activity>
        <activity
            android:name=".MDetalle.centroListActivity"
            android:label="@string/title_centro_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MDetalle.centroDetailActivity"
            android:label="@string/title_centro_detail"
            android:parentActivityName=".MDetalle.centroListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.sanapruebados.MDetalle.centroListActivity" />
        </activity>
        <activity
            android:name=".MDetalle.AdiccionListActivity"
            android:label="@string/title_adiccion_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MDetalle.AdiccionDetailActivity"
            android:label="@string/title_adiccion_detail"
            android:parentActivityName=".MDetalle.AdiccionListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.sanapruebados.MDetalle.AdiccionListActivity" />
        </activity>
        <activity
            android:name=".miInicio"
            android:label="@string/title_activity_mi_inicio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".establecimientos" />
        <activity android:name=".adicciones" />
        <activity android:name=".inicio" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--android:authorities="com.example.sanapruebados" -->
        <!--android:authorities="${applicationId}.provider"-->
        <provider
        android:name="android.support.v4.content.FileProvider"
        android:authorities="${applicationId}"
        android:exported="false"
        android:grantUriPermissions="true">
        <meta-data
        android:name="android.support.FILE_PROVIDER_PATHS"
        android:resource="@xml/file_paths">
        </meta-data>
        </provider>

    </application>

</manifest>